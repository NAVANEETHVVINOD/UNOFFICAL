{
  "name": "college-connect",
  "version": "0.1.0",
  "private": true,
  "packageManager": "npm@10.2.3",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md,css,html}\"",
    "clean": "turbo run clean"
  },
  "devDependencies": {
    "@types/node": "^24.9.2",
    "eslint": "^9.39.0",
    "prettier": "^3.6.2",
    "turbo": "^2.6.0",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": "20.x"
  },
  "dependencies": {
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "framer-motion": "^12.23.24"
  }
}
